    "version": "1.22.5",
    "suggest": {
        "Node.js": [
            "nodejs",
            "nodejs-lts"
        ],
        "NVM": "dorado/nvm-windows"
    },
    "url": "https://yarnpkg.com/downloads/1.22.5/yarn-1.22.5.msi",
