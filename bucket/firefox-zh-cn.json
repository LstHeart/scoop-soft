            "hash": "10451b670e957cd158934d72ec6c9f6dfc0e330f0edc8dd4ed228ba604a64a44"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-90.0.2-SSL&os=win&lang=zh-CN#/firefox.7z",
