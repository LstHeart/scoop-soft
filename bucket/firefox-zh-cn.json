    "architecture": {
        "64bit": {
            "url": "https://download.mozilla.org/?product=firefox-91.0.2-SSL&os=win64&lang=zh-CN#/firefox.7z",
            "hash": "a9cf04ade0644cd3dfc9177bc9a38ca3c52144e7fea886e644689214221f2df4"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-91.0.2-SSL&os=win&lang=zh-CN#/firefox.7z",
            "hash": "c67e0e7f3234e057aed79de578c1d19c79add8603e46a4124ac60a7bb0060c77"
        }
    },
    "extract_dir": "core",
    "bin": "firefox.exe",
    "shortcuts": [
        [
            "firefox.exe",
            "Mozilla Firefox"
        ]
    ],
    "checkver": {
        "url": "https://www.mozilla.org/zh-CN/firefox/all/",
        "re": "data-latest-firefox=\"([\\d.]+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win64&lang=zh-CN#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win64/zh-CN/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            },
            "32bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win&lang=zh-CN#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win32/zh-CN/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            }
        }
    }
}
