                }
            },
            "32bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win&lang=zh-CN#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win32/zh-CN/Firefox Setup ([\\d.]+).exe)",
