    "architecture": {
        "64bit": {
            "url": "https://download.mozilla.org/?product=firefox-96.0.1-SSL&os=win64&lang=zh-CN#/firefox.7z",
            "hash": "bbeb3953c11a81a97c25b4d43f109e2e264bc02a327ce8da9717f76fd69fa85c"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-96.0.1-SSL&os=win&lang=zh-CN#/firefox.7z",
            "hash": "896d28bc5f6f20f7a5b2469a4949ceaa62d1c2c86dacf6d73cb407310145410b"
        }
    },
    "extract_dir": "core",
    "bin": "firefox.exe",
    "shortcuts": [
        [
            "firefox.exe",
            "Mozilla Firefox"
        ]
    ],
    "checkver": {
        "url": "https://www.mozilla.org/zh-CN/firefox/all/",
        "re": "data-latest-firefox=\"([\\d.]+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win64&lang=zh-CN#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win64/zh-CN/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            },
            "32bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win&lang=zh-CN#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win32/zh-CN/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            }
        }
    }
}
